	keywords = {Quality of life, Healthy ageing, Social infrastructure, Urban green space access, Urban green space design, Urban green space management},
	pages = {203--213},
	file = {ScienceDirect Snapshot:C\:\\Users\\jodyh\\Zotero\\storage\\TISPNLB9\\S1618866717301577.html:text/html},
